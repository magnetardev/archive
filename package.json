{
	"name": "@magnetardev/archive",
	"version": "2.0.0",
	"description": "A simpler archive library for the web.",
	"repository": "https://github.com/magnetardev/archive.git",
	"author": "Magnetar <contact@magnetar.dev>",
	"license": "MIT",
	"packageManager": "yarn@3.2.2",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"main": "./dist/index.umd.cjs",
	"module": "./dist/index.mjs",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.umd.cjs"
		}
	},
	"scripts": {
		"build:wasm": "cd lib && make ../src/archive.js && cd ..",
		"dev": "vite",
		"build": "tsc && vite build",
		"fmt": "prettier -w .",
		"test": "vitest run"
	},
	"devDependencies": {
		"@types/node": "^18.6.5",
		"prettier": "^2.7.1",
		"typescript": "^4.6.4",
		"vite": "^3.0.0",
		"vite-plugin-dts": "^1.4.1",
		"vitest": "^0.21.0"
	}
}
